{
  "manifest_version": 2,
  "name": "GHFilesChecker",
  "description": "☑️ Check all the files already reviewed for a pull request.",
  "version": "0.1.0",
  "author": "Helder Burato Berto",
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*"],
      "css": ["style.css"],
      "js": ["main.js"]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
